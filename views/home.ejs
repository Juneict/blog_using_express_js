<% if(blogs.length) { %> <% blogs.forEach(blog =>{%>
<div class="blog-card">
  <a href="/blogs/<%= blog.id %>"><h1><%= blog.title %></h1></a>
  <p><%= blog.intro %></p>
  <form action="/blogs/<%= blog.id %>/delete" method="POST">
    <button type="submit" class="delete-blog-btn">delete</button>
  </form>
</div>
<% }) %> <% } else { %> <% } %>
